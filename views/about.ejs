<!DOCTYPE html>
<html lang="en">
    <%-include('./partials/head.ejs')%>
<body>
    <h1>About Page</h1>
    <%-include('./partials/nav.ejs')%>
    <div>
    <% if (users.length > 0) {%>
        <% users.forEach(users => { %>
        <a class="single" href="/users/<%=users._id%>">
            <h1><%=users.username%></h1>
            <h6><%=users.password%></h6>
        </a>
        <%})%>
        <% } else { %>
            <p>There are no blogs to display...</p>
          <%} %>
    
    </div>

    <%-include('./partials/footer.ejs')%>
    
</body>
</html>